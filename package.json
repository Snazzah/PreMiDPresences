    "axios": "^0.24.0",
    "chalk": "^4.1.2",
    "debug": "^4.3.2",
    "enquirer": "^2.3.6",
    "eslint": "^7.32.0",
    "eslint-plugin-eqeqeq-fix": "^1.0.3",
    "glob": "^7.2.0",
    "jsonschema": "^1.4.0",
    "json-to-ast": "^2.1.0",
    "mongodb": "^3.6.9",
    "ora": "^6.0.1",
    "prettier": "^2.4.1",
    "semver": "^7.3.5",
    "source-map-support": "^0.5.19",
    "terser": "^5.9.0",
    "typescript": "^4.4.3"
