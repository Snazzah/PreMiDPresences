    "@types/debug": "^4.1.7",
    "@types/glob": "^7.1.4",
    "@types/mongodb": "4.0.7",
    "@types/node": "^17.0.17",
    "@types/prettier": "^2.4.1",
    "@types/semver": "^7.3.8",
